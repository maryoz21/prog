def generate_serie_6(n:int):
    last_number = 0
    number = 1
    suma = 0
    result = [last_number, number]
    i = 2
    if n < 0:
        return []
    while i < n:
        suma = number + last_number
        last_number = number
        number = suma
        result.append(suma)
        i += 1
    return result
list = generate_serie_6(-6)
print(list)